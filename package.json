{
  "name": "wordup-server",
  "version": "1.0.0",
  "description": "Server portion of wordup app",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "NODE_ENV='production' git checkout -b deploy && webpack -p && git add -f public/bundle.js public/bundle.js.map && git commit --allow-empty -m 'Deploying' && git push --force heroku deploy:master && git checkout master && git branch -D deploy",
    "start": "node server",
    "start-dev": "npm run build-client-watch & npm run start-server",
    "start-server": "NODE_ENV='development' nodemon server -e html,js,scss --ignore public",
    "test": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/wordup-test' mocha ./**/*.spec.js --compilers js:babel-register",
    "seed": "node seed",
    "prepWordData": "node prepWordData"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.16.1",
    "chance": "^1.0.11",
    "connect-session-sequelize": "^4.1.0",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "history": "^4.6.3",
    "konva": "^1.6.8",
    "lodash": "^4.17.4",
    "morgan": "^1.8.1",
    "neo4j": "^2.0.0-RC2",
    "neo4j-driver": "^1.2.0",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "promise-retry": "^1.1.1",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-konva": "^1.1.4",
    "react-redux": "^5.0.2",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.1",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.74.2",
    "sequelize": "^4.3.1",
    "socket.io": "^2.0.3",
    "unirest": "^0.5.1"
